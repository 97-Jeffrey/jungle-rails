<section class="user-login">
  <h1>Login</h1>
  <div class="form-group">
    <%= form_tag '/login' do %>

      Email: <%= text_field_tag :email %>
      Password: <%= password_field_tag :password %>
      <button type="submit" class="btn btn-primary" <%= submit_tag %>Submit</button>

    <% end %>
  </div>
</section>